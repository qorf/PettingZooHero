use Templates.PettingZooHero.all

Application myGame
Level level1

Animal animal
animal:SetImageFile("Bunny.png")
animal:SetSpeed(2)
animal:SetHP(2)
animal:SetName("Big Bunny")
animal:SetDescription("This guy's fast and hungry!")
level1:AddAnimal(animal, 2, 2)

integer x = 0
repeat while x < 5
    level1:AddSheep(x, 4)
    x = x + 1
end

myGame:AddLevel(level1)
myGame:Run()

